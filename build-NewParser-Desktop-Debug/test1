read,READ
x,IDENTIFIER
;,SEMICOLON
if,IF
x,IDENTEFIER
<,LESSTHAN
0,NUMBER
then,THEN
repeat,REPEAT
y,IDENTIFIER
:=,ASSIGN
z,IDENTIFIER
+,PLUS
1,NUMBER
;,SEMICOLON
z,IDENTIFER
:=,ASSIGN
z,IDENTIFIER
*,MULT
2,NUMBER
until,UNTIL
z,IDENTIFIER
=,EQUAL
20,NUMBER
;,SEMICOLON
write,WRITE
y,IDENTIFIER
end,END
